<div>
	<h3>Users list</h3>
	<div>
		<table>
			<tr>
				<th>User Name</th>
				<th>Address</th>
				<th>PH No</th>
			</tr>
			<% @users.each do|user| %>
			<tr>
				<td><%= user.id %></td>
				<td><%= user.name %></td>
				<td><%= user.address %></td>
				<td><%= user.phone %></td>
				<td><%= link_to "Edit", edit_user_url(user.id)%></td>
				<td><%= link_to "Delete", user_url(user.id), method: :delete, data:{confirm: "Are yu sure to Dtelet"} %></td>
			</tr>

			<% end %>
			<tr>
				<td><%= link_to "new", new_user_url %></td>
			</tr>
		</table>
	</div>
</div>